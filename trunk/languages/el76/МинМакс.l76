программа
  конст M = 20, N = 20;
  ф32 матр := лок вект[M:N] ф32;
  ф32 i,j, мин, макс;
  для i до M-1 цикл
    для j до N-1 цикл
      матр[i,j] := случ()*10.0
    повторить
  повторить;
  мин := матр[0,0];
  макс := матр[0,0];
  для i до M-1 цикл
    для j до N-1 цикл
      если матр[i,j] < мин то мин := матр[i,j] все;
      если матр[i,j] > макс то макс := матр[i,j] все
    повторить
  повторить;
  печать(мин);
  печать(макс)
конец
