программа
  процедура пр1 = проц(ф32 а,б,в)
    начало 
      печать(а);
      печать(б);
      печать(в[0,0]);
      в[1,1] := 999
    конец;
  процедура пр2 = функция(ф32 к)(к*к);
  ф32 масс := лок вект[2:2] ф32;
  печатькс();
  печать(масс[0,0]);
  пр1(1,2,масс);
  печать(масс[1,1])
  %печать(пр2(масс[1,1]))  
конец
