программа
  конст
    N = 1000; %* Число разбиений %*
  процедура
    f = функция(ф32 x)(x*x*x*x);
  ф32 i,
    minX := 0.0, maxX := 1.0,
    dx := (maxX - minX)/вещокр N,
    s := 0.0, y := f(minX);
  для i от 1 до N цикл
    ф32 x := minX + (вещобр i)*dx,
        y1 := f(x);
    s := s + dx / 2.0 * (y + y1);
    y := y1
  повторить;
  печать(s);
  печатькс()
конец
